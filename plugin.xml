<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-SaveImgToAlbum" version="1.0" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>SaveImgToAlbum</name>
    <js-module name="SaveImgToAlbum" src="www/SaveImgToAlbum.js">
        <clobbers target="cordova.plugins.SaveImgToAlbum" />
    </js-module><platform name="ios">
        <config-file parent="/*" target="config.xml">
            <feature name="SaveImgToAlbum">
                <param name="ios-package" value="SaveImgToAlbum" />
            </feature>
        </config-file>
        <header-file src="src/ios/SaveImgToAlbum.h" />
        <source-file src="src/ios/SaveImgToAlbum.m" />
    </platform></plugin>
